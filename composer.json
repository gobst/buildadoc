{
  "name": "gobst/buildadoc",
  "description": "BuildADoc is a PHP documentation generator designed specifically for DokuWiki.",
  "type": "library",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Guido Obst",
      "role": "Web Application Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/gobst/buildadoc/issues"
  },
  "autoload": {
    "psr-4": {
      "Command\\": "src/Command/",
      "Generator\\": "src/Generator/",
      "Exception\\": "src/Exception/",
      "Service\\": "src/Service/",
      "Contract\\": "src/Contract/",
      "Decorator\\": "src/Decorator/",
      "Adapter\\": "src/Adapter/",
      "Pipeline\\": "src/Pipeline/",
      "": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "": "tests/data/classes/"
    }
  },
  "require": {
    "php": ">=8.3",
    "symfony/console": "^v6.4",
    "symfony/filesystem": "^v6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/config": "^v6.4",
    "symfony/process": "^7.1",
    "nikic/php-parser": "^v4.19",
    "illuminate/collections": "^v11.17",
    "illuminate/pipeline": "^11.19",
    "laminas/laminas-text": "^2.11"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "infection/infection": "^0.27"
  },
  "prefer-stable": true,
  "config": {
    "bin-dir": "bin",
    "allow-plugins": {
      "infection/extension-installer": false
    }
  }
}
